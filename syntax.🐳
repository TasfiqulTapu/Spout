let a = 0b123 // binary
let b = 0o123 // octal
let c = 0x123 // hexadecimal
let d = 1234 // decimal
let e = 1_234 // underscore for readability, gets removed
let f = 'a' // single quotes for characters

// Bitwise operators
let g = 0b1001
let h = 0b0101
let i = g & h // 0b0001
let j = g | h // 0b1101
let k = g ^ h // 0b1100
let l = ~g // 0b11111111111111
let m = g << 1 // 0b10010
let n = g >> 1 // 0b0100
let o = g >>> 1 // 0b0100

// math operators
let p = 1 + 2
let q = 1 - 2
let r = 1 * 2
let s = 1 / 2
let t = 1 % 2
let u = 1 ** 2



// outputting
spout("%b", g) // 1001

// Inputting
let p = snort("%b")
